@page
@model IndexModel
@{
    ViewData["Title"] = "Events";
}

<h1>Events</h1>

@foreach (var e in Model.Events)
{
    <div>
        <h4>@e.Name</h4>
        <p>@e.Description</p>
        <p>Start Time: @e.StartTime.ToString("yyyy-MM-dd HH:mm")</p>
        <p>End Time: @e.EndTime.ToString("yyyy-MM-dd HH:mm")</p>
    </div>
}